{
  "name": "iress-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5020 --strictPort",
    "preview": "vite preview --port 8080 --strictPort",
    "serve": "vite preview --port 5020 --strictPort",
    "build": "vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "fix": "eslint src --ext .js,.ts,.tsx --max-warnings 0 --fix",
    "prettier": "prettier --write src/",
    "buildAll": "(yarn build);(yarn build:internal)",
    "test": "export TS_JEST_DISABLE_VER_CHECKER=true && jest --runInBand"
  },
  "dependencies": {
    "iress-ui-lib": "/Users/xiaoyuhan/projects/iress/iress/design-system/iress-ui-lib-v1.0.0.tgz",
    "antd": "^5.7.3",
    "axios": "^1.4.0",
    "core-js": "^3.30.2",
    "dotenv": "^16.3.1",
    "jotai": "^2.0.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.0",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "styled-components": "^5.3.11",
    "vite-plugin-babel-macros": "^1.0.6"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.2.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@remix-run/router": "^1.0.2",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.198",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.0.11",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^3.0.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "flush-promises": "^1.0.2",
    "husky": "^8.0.0",
    "jest": "^27.0.5",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "msw": "^1.2.1",
    "prettier": "^2.8.8",
    "ts-jest": "^27.0.5",
    "typescript": "^5.0.4",
    "uuid": "^8.3.2",
    "vite": "^4.3.2",
    "vite-plugin-html": "^3.2.0",
    "vitest": "^0.34.3"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "lint-staged": {
    "!(*d).(ts|tsx)": [
      "prettier --config .prettierrc --write src/",
      "eslint --max-warnings 0 --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/test/**/*",
      "!src/setupTests.ts",
      "!**/{mock,mocks}.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "testMatch": [
      "**/*test.(ts|tsx)"
    ]
  }
}
